{
  "name": "simplon-ville-dashboard",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:electron": "ng build --base-href ./ && electron .",
    "package:win": "npm run build && cross-var electron-packager ./dist/SimplonVilleDashboard $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite ",
  "package:linux": "npm run build && cross-var electron-packager ./ $npm_package_name-$npm_package_version --out=packages --platform=linux --arch=all --overwrite ",
  "package:osx": "npm run build && cross-var electron-packager dist/SimplonVilleDashboard $npm_package_name-$npm_package_version --out=packages --platform=darwin --arch=all --overwrite ",
  "package:all": "npm run build && cross-var electron-packager ./ $npm_package_name-$npm_package_version --out=packages --all --arch=all --overwrite "
  },
  
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "bootstrap": "^5.1.3",
    "express": "^4.17.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@angular/localize": "^12.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "cross-var": "^1.1.0",
    "electron": "^16.0.4",
    "electron-packager": "^15.4.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}
